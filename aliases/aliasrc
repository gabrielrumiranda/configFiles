alias aliasrc='$EDITOR $HOME/aliases/aliasrc'

# zsh Files:
alias zshrc='$EDITOR $HOME/.zshrc'
alias zprofile='$EDITOR $HOME/.zprofile'
alias zshenv='$EDITOR $HOME/.zshenv'
alias update='sudo apt update && sudo apt upgrade -y && sudo apt clean && sudo apt autoclean && sudo apt autoremove -y'
alias icat="kitty +kitten icat"
